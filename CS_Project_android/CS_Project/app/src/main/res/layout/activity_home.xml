<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".HomeActivity">

    <Button
        android:id="@+id/btn_os"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="200dp"
        android:background="@drawable/btn_change"
        android:fontFamily="@font/nanumsquareroundb"
        android:text="운영체제"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_com"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/nanumsquareroundb"
        android:text="컴퓨터 구조"
        android:textSize="20sp"
        android:background="@drawable/btn_change"
        app:layout_constraintEnd_toEndOf="@+id/btn_os"
        app:layout_constraintStart_toStartOf="@+id/btn_os"
        app:layout_constraintTop_toBottomOf="@+id/btn_os" />

    <Button
        android:id="@+id/btn_sum"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/nanumsquareroundb"
        android:text="종합문제"
        android:textSize="20sp"
        android:background="@drawable/btn_change"
        app:layout_constraintEnd_toEndOf="@+id/btn_com"
        app:layout_constraintStart_toStartOf="@+id/btn_com"
        app:layout_constraintTop_toBottomOf="@+id/btn_com" />
</androidx.constraintlayout.widget.ConstraintLayout>